generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model Carros {
  id Int      @id @default(autoincrement())
  placa String
  marca String
  modelo String
  Ano_do_ve√≠culo Int
  clientes Clientes[]
}

model Clientes {
  id Int    @id @default(autoincrement())
  nome String
  cpf Int
  email String
  cnh String
  carros Carros @relation(fields: [carrosId], references: [id])
  carrosId Int
}
